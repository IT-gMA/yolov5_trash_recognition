Bootstrap: docker
From: pytorch/pytorch:1.10.0-cuda11.3-cudnn8-runtime

%post
    apt-get update -y && apt-get install -y libgtk2.0-dev

    python3 -m pip --no-cache-dir install \
    numpy \
    opencv-python \
    opencv-python-headless \
    packaging \
    Pillow \
    torchaudio \
    torchvision \
    -r requirements.txt \
    tensorboard \
